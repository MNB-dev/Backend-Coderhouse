<html>
  <head>
    <meta charset="utf-8" />
    <title>Express</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <div class="div"></div>

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.7.7/handlebars.min.js"
    ></script>
    <script>
      const template = Handlebars.compile(`<div class="container"> <h1
      class="m-3">Welcome</h1> <form action="/api/productos" method="post"> <div
      class="mb-3"> <label for="FormControlInput1"
      class="form-label">Titulo</label> <input type="text" name="title"
      class="form-control" id="FormControlInput1" placeholder="Ingrese un titulo" /> </div> <div class="mb-3"> <label for="FormControlInput2"
      class="form-label">Thumbnail</label> <input type="text" name="thumbnail"
      class="form-control" id="FormControlInput2" placeholder="Ingrese un thumbnail" /> </div> <div class="mb-3"> <label for="FormControlInput3"
      class="form-label">Precio</label> <input type="text" name="price"
      class="form-control" id="FormControlInput3" placeholder="Ingrese un precio" /> </div> <button class="btn btn-primary">Agregar</button> </form>
      <div class="d-grid gap-2 d-md-flex justify-content-md-end"> <form
      action="/api/productos" method="get"> <button class="btn
      btn-outline-primary">Obtener productos</button> </form> </div> </div>`);
      const html = template({ nombre: 'home' });
      document.querySelector('div').innerHTML = html;
    </script>
  </body>
</html>